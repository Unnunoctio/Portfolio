---
import type { Resume } from '../types';
import ResumeItem from './ResumeItem.astro';

interface Props {
  items: Resume[];
  style: string;
}

const { items, style } = Astro.props;
---

<ul class={style}>
  {items.map((item, index) => (
    <ResumeItem {...item} end={index === items.length - 1} />
  ))}
</ul>